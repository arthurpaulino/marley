#this file is required to chain the autoconf run in ../ with the cmake run in build/
SUBDIRS = build

all: build
	$(info   *************** ppsspp all end ***************)
install:
	$(info   *************** install checkpoint ***************)
build:
	$(info   *************** ppsspp build ***************)
	cd ffmpeg  && ./linux_x86-64.sh &&cd ..
	$(MAKE) -C $@

